public class Outlier {
    public static int find(int[] integers) {
        int evenCount = 0;
        int oddCount = 0;
        int even = 0;
        int odd = 0;
        
        for (int i : integers) {
            if (i % 2 == 0) {
                evenCount++;
                even = i;
            } else {
                oddCount++;
                odd = i;
            }
        }
        
        return evenCount == 1 ? even : odd;
    }

    public static public void main(String[] args) {
        int[] integers = {2, 4, 0, 100, 4, 11, 2602, 36};
        System.out.println(find(integers));
    }
}
