public class FindNb {
    public static long findNb(long m) {
        long n = 1;
        long sum = 0;
        while (sum < m) {
            sum += (long) Math.pow(n, 3);
            if (sum == m) {
                return n;
            }
            n++;
        }
        return -1;
    }
